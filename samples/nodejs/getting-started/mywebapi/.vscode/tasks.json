{
    "tasks": [
        {
            "label": "vsce: ready",
            "command": "vsce",
            "args": [
                "up",
                "--iterate",
                "--port",
                "53701:9229",
                "--no-command",
                "--config",
                "develop",
                "--no-telemetry",
                "--",
                "npm",
                "start"
            ],
            "options": {
                "cwd": "${workspaceRoot}"
            },
            "problemMatcher": []
        },
        {
            "label": "nodemon: server.js (VSCE)",
            "command": "vsce",
            "args": [
                "up",
                "--iterate",
                "--port",
                "53701:9229",
                "--config",
                "develop.attach",
                "--no-telemetry",
                "--",
                "node_modules/.bin/nodemon",
                "--inspect=0.0.0.0:9229",
                "--nolazy",
                "server.js"
            ],
            "options": {
                "cwd": "${workspaceRoot}"
            },
            "problemMatcher": []
        }
    ]
}